{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:weekly",
  ],
  "labels": ["chore"],
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchPaths": [".github/**"],
      "enabled": true,
      "groupName": "github-actions"
    },
  ]
}
